<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aplayer/dist/APlayer.min.css">
<script src="https://cdn.jsdelivr.net/npm/aplayer/dist/APlayer.min.js"></script>
<!-- require MetingJS -->
<script src="https://cdn.jsdelivr.net/npm/meting@2.0.1/dist/Meting.min.js"></script>
<meting-js 
	server="<%= theme.music.server %>" 
	type="<%= theme.music.type %>" 
	id="<%= theme.music.id %>"
	<% if (theme.music.mini){ %>
	mini="<%= theme.music.mini %>"
	<% } %>
	<% if (theme.music.fixed){ %>
	fixed="<%= theme.music.fixed %>" 
	<% } %>
	autoplay="<%= theme.music.autoplay %>"
	loop="<%= theme.music.loop %>"
	order="<%= theme.music.order %>"
	preload="<%= theme.music.preload %>"
	list-folded="<%= theme.music.listfolded %>">
</meting-js>