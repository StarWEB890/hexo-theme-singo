<section class='m_widget links animate__animated <%= theme.animate.sidebar %>'>
<div class='header'><i class="fa fa-link" aria-hidden="true"></i><%- __('sidebar.social') %></div>
<div class='content'>
    <ul class="entry">
    <% (theme.social||[]).forEach(function(item){ %>
        <li class="side-<%= item.name %>"><a class="flat-box" target="_blank" href="<%= url_for(item.url) %>">
            <div class='name'><i class="<%= item.slug %>" aria-hidden="true"></i>&nbsp;&nbsp;<%= item.name %></div>
        </a></li>
    <% }); %>
    </ul>
</div>
</section>